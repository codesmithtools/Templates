						Dim <%= GetCamelCaseName(GetRowClassName()) %> As <%= GetRowClassName() %> = New <%= GetRowClassName() %>(reader)
						<%= GetRowClassCollectionInstanceName() %>.Add(<%= GetCamelCaseName(GetRowClassName()) %>)
					End While
				Finally
